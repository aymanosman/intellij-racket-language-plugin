<idea-plugin>
    <id>org.racket.aymanosman</id>
    <name>Racket</name>
    <vendor url="https://github.com/aymanosman">Ayman Osman</vendor>

    <description><![CDATA[
    Provides support for the <a href="https://racket-lang.org/">Racket</a> programming language.<br>

    Features:<br>
      <ul>
          <li>Syntax Highlighting</li>
      </ul>
    ]]></description>

    <extensions defaultExtensionNs="com.intellij">
        <fileType
                name="Racket file"
                implementationClass="org.racket.lang.core.parser.RacketFileType"
                fieldName="INSTANCE"
                language="Racket"
                extensions="rkt"/>
        <lang.parserDefinition
                language="Racket"
                implementationClass="org.racket.lang.core.parser.RacketParserDefinition"/>
        <lang.syntaxHighlighterFactory
                language="Racket"
                implementationClass="org.racket.lang.core.parser.RacketSyntaxHighlighterFactory"/>
        <colorSettingsPage
                implementation="org.racket.lang.core.parser.RacketColorSettingsPage"/>
        <annotator
                language="Racket"
                implementationClass="org.racket.ide.highlight.RacketSyntaxHighlightAnnotator"/>
    </extensions>

    <actions>
        <!-- Add your actions here -->
    </actions>
</idea-plugin>
